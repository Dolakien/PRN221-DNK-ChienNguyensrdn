@{
    ViewData["Title"] = "History";
    @using HealthyMomAndBaby;
    @using HealthyMomAndBaby.Models
    @using HealthyMomAndBaby.Entity
    @addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
}
@model IEnumerable<Order>
<!DOCTYPE html>
<bo lang="en">
  <head>
    <title>Title</title>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />

    <!-- Bootstrap CSS v5.2.1 -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
  </head>

  <body style="background-color: #f7f7f7; width: 100vw; overflow-x: hidden;" >
    <div
    style="height: 64px; background-color: white"
  >
  
  <div class="">
    <div class="row">
      <div class="col-2 d-flex align-items-center"   style="height: 64px;">
        <img
      src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTuNlrvrFMLORKsLtoNNG6J0ulGeOgOBuzcNw&s"
      alt=""
      width="40"
      height="40"
      class="rounded-circle mx-4"
    />
    <h3>Trần Thanh Sơn</h3>
      </div>
      <div class="col-2">
      </div>
      <div class="col-2">
      </div>
      <div class="col-2">
      </div>
      <div class="col-1">
      </div>
      <div class="col-1">
      </div>
      <div class="col-2 d-flex justify-content-end align-items-center "  style="height: 64px;">
        <div>
          <a href=""><i class="fa-solid fa-cart-shopping" style="font-size: 30px;"></i></a></div>
          <button class="btn btn-danger" style="margin-left: 20px; margin-right: 20px;">
            Logout
          </button>
        </div>
      
      </div>
    </div>
  </div>
  </div>
<!-- hình động-->

<div class="container rounded" style="height: 68vh; background-color: white; margin-top: 20px;">
   <table class="table">
        <tr>
            <th>Order Code</th>
            <th>Full Name</th>
            <th>Ship Address</th>
            <th>Phone Number</th>
            <th>Total Price</th>
        </tr>
          @foreach (var order in Model) {
        <tr>
            <td>@order.Id</td>
            <td>@order.FullName</td>
            <td>@order.ShipAddress</td>
            <td>@order.PhoneNumber</td>
            <td>@order.Total</td>
       </tr>
          }
   </table>
</div>

  <!-- Footer-->
   <div style="height: 200px; background-color: #0d99ff; margin-top: 20px;">

   </div>
  </body>
 
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
</html>
